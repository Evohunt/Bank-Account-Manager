            <Font name="Impact" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="registerButton" alignment="CENTER" layoutX="170.0" layoutY="258.0" onMouseClicked="#gotoRegisterScreen" prefHeight="55.0" prefWidth="261.0" styleClass="registerButton" stylesheets="@../CSS/buttons.css" text="REGISTER" textFill="WHITE">
